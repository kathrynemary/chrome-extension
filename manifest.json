{
  "manifest_version": 2,
  "name": "My Test Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ], 
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
